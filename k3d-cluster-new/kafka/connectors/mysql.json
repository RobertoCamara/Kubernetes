{
  "name": "mysql-clientes-connector",
  "config": {
    "connector.class": "io.debezium.connector.mysql.MySqlConnector",
    "database.hostname": "172.28.243.188",
    "database.port": "3306",
    "database.user": "root",
    "database.password": "mysqldb",
    "database.server.id": "184054",
    "database.include.list": "kafka",
    "table.include.list": "kafka.clientes",
    "include.schema.changes": "true",
    "topic.prefix": "kafka",
    "decimal.handling.mode": "double",
    "tasks.max": "1",
    "key.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "header.converter": "org.apache.kafka.connect.json.JsonConverter",
    "errors.retry.timeout": "0",
    "errors.retry.delay.max.ms": "60000",
    "errors.tolerance": "none",
    "errors.log.enable": "false",
    "errors.log.include.messages": "false",
    "snapshot.mode": "never",
    "snapshot.locking.mode": "minimal",
    "bigint.unsigned.handling.mode": "long",
    "time.precision.mode": "adaptive_time_microseconds",
    "enable.time.adjuster": "true",
    "binary.handling.mode": "bytes",
    "tombstones.on.delete": "true",
    "heartbeat.interval.ms": "0",
    "heartbeat.topics.prefix": "__debezium-heartbeat",
    "schema.history.internal": "io.debezium.storage.kafka.history.KafkaSchemaHistory",
    "schema.history.internal.kafka.bootstrap.servers": "kafka.kafka.svc.cluster.local:9092",
    "schema.history.internal.kafka.topic": "schema-changes.mysql-kafka"
  }
}
